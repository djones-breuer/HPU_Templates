<!doctype html>
<head>
	<meta http-equiv="Content-type" content="text/html; charset=utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />

	<title>Family Update Profile | Templates</title>

	<link href="/style/font-awesome.css" media="all" rel="stylesheet" type="text/css"></link>
	<link href="/style/font-awesome-ie7.css" media="all" rel="stylesheet" type="text/css"></link>
	<link href="/style/style.css" media="all" rel="stylesheet" type="text/css"></link>

	<script src="http://code.jquery.com/jquery-latest.js" type="text/javascript"></script>
	<script src="/script/app.js" type="text/javascript"></script>
	
</head>
<body class="" ontouchstart="">

	<ul id="temp-demo">
		<li><a href="#" id="toggle_cards"> Toggle Cards</a></li>
	</ul>

	<div id="household" class="container">

		<div id="household-profile" class="content">

			<div id="insert-card-contact" class='new-card card'>
	
			 

				<div class='card-content read-content edit-toggle'>
			    <form>
			      <input type="hidden" name="record_id" value="0"></input>
			      <input type="hidden" name="table_name" value="Person"></input>
			      
			     <p class="label mini-section">Name &amp; Relationships: </p>

					  <dl>

			  			<dt>Prefix:</dt>
			  			<dd>
			          <select name="name_prefix">
			            <option>Mr</option>
			            <option>Mrs</option>
			          </select>
			        </dd>

			        <dt>First Name:</dt>
			        <dd><input type="text" /></dd>

			      </dl>

			      <p class="label mini-section">Contact Information</p>
			      <dl>
			        
			          <dt>Home Phone:</dt>
			          <dd><input type="text" /></dd>
			        
			    
			       
			          <dt>Work Phone:</dt>
			          <dd><input type="text" /></dd>
			       
			 
			  		</dl>

					 
			    </form>
				  
			 
				</div>

			</div>


		</div><!-- Content -->
	</div><!-- Container -->

	<div id="overlay"></div>
</body>
</html>